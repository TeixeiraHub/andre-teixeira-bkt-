package candidatura.questao2;

import java.util.Random;

public class Questao2 {

	public static void main(String[] args) {
		Random gerador = new Random();

		int numGerado1 = gerador.nextInt(100);
		
		int numGerado2 = gerador.nextInt(10);

		System.out.println(numGerado1);
		System.out.println();
		System.out.println(numGerado2);
		System.out.println();
		for (int i = 1; i <= numGerado1; i++) {
			if (i%2==0 && i%5!=0) {
				System.out.println(i);		
			}			
		}
	}

}
