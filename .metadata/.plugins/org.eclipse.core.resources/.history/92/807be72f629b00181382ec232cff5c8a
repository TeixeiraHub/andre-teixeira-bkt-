package mat.comp;

public class NewtonRaphson {
	
	public static double horner (double[] a, double x) {  
	     double result = 0.0;  
	     int n = a.length - 1;  
	     for (int i = n; i >= 0; i--) {  
	                 
	         result = x * result + a [i]; 
	     }  
	     return result;  
	}
	
	public static void main(String[] args) {

		double[] a = { 1, 30, 7, 12, 23, 50 }; 
		
		NewtonRaphson nr = new NewtonRaphson;
          
		horner(a, 4);  
	}

}
