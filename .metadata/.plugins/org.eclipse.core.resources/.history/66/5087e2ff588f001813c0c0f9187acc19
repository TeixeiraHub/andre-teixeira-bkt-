package candidatura.questao1;

import java.util.Random;

public class Questao1 {

	public static void main(String[] args) {
		System.out.println(gerarPar());
	}
	
		
		static int gerarPar(){
			
			int numeroPar = 0;
			Random gerador = new Random();
			
			int numGerado = gerador.nextInt();
			for (int i = 1; i <= numGerado; i++) {
				if (numGerado%2==0 && numGerado%5!=0);
				numeroPar = numGerado;
				
				System.out.println(numeroPar);
							
			}
			return numeroPar; 
		};

	}


